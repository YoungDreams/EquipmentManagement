
@{
    Layout = "../Shared/_Layout.cshtml";
}


<div id="app">
    <el-button v-on:click="visible = true">Button</el-button>
    <el-dialog :visible.sync="visible" title="Hello world">
        <p>Try Element</p>
    </el-dialog>
</div>

<!-- import Vue before Element -->
@section scripts{
    <script>
        new Vue({
            el: '#app',
            data: function () {
                return { visible: false }
            }
        })
    </script>
}

