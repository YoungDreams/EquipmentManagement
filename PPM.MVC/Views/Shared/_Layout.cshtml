    <!-- 引入样式 -->
    @*<link rel="stylesheet" href="https://unpkg.com/element-ui/lib/theme-chalk/index.css">*@
    <!-- 引入组件库 -->
    @*<script src="https://unpkg.com/vue/dist/vue.js"></script>
        <script src="https://unpkg.com/element-ui/lib/index.js"></script>*@
    <script src=@Url.Content("~/assets/global/plugins/jquery.min.js") type="text/javascript"></script>
    
</head>
<body class="page-header-fixed page-footer-fixed page-sidebar-closed-hide-logo page-content-white">
    @Html.Partial("NavBar")
    <div class="page-container">
        @Html.Partial("SideBar")
        <div class="page-content-wrapper">
            <!-- BEGIN CONTENT BODY -->
            <div class="page-content" style="min-height:895px">
                <!-- BEGIN PAGE HEADER-->
                <!-- BEGIN THEME PANEL -->
                <!-- END THEME PANEL -->
                <!-- BEGIN PAGE BAR -->
                <div class="page-bar">
                    <!-- 此处是相关代码 -->
                    <ul class="nav nav-tabs" role="tablist"></ul>
                    <div class="tab-content" style="width:100%;">
                    </div>
                    <!-- 相关代码结束 -->
                </div>
                <!-- END PAGE BAR -->
                <!-- BEGIN PAGE TITLE-->
                <h3 class="page-title">
                    @RenderSection("PageTitle", false)
                </h3>
                <!-- END PAGE TITLE-->
                <!-- END PAGE HEADER-->
                <div id="#content">
                    @RenderBody()
                </div>
            </div>
            <!-- END CONTENT BODY -->
        </div>
    </div>
    <div class="page-footer">
        <div class="page-footer-inner">
            &copy; @DateTime.Now.Year - Copyright Reserved.
        </div>

        <div class="scroll-to-top" style="display: block;">
            <i class="glyphicon glyphicon-plane"></i>
        </div>
    </div>
    <!--[if lt IE 9]>
        <script src=@Url.Content("~/assets/global/plugins/respond.min.js")></script>
