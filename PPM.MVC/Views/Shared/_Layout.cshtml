    <script src=@Url.Content("~/assets/app.js") type="text/javascript"></script>
    @*<script src=@Url.Content("~/assets/bootstrap-closable-tab.js") type="text/javascript"></script>*@
    <script src=@Url.Content("~/assets/pages/scripts/ui-tree.js") type="text/javascript"></script>
    <script>
        $.fn.datetimepicker.dates['zh'] = {    
            days:       ["星期日", "星期一", "星期二", "星期三", "星期四", "星期五", "星期六","星期日"],    
            daysShort:  ["日", "一", "二", "三", "四", "五", "六","日"],    
