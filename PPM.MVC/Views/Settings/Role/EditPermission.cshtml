@section scripts{

    <script type="text/javascript">
        var funcs = {
            getPermissions: function() {
                var permissions = [];
                var checkboxes = $(".checkboxes");
                $.each(checkboxes, function(i, item) {
                    var chk = $(item);
                    if (chk.is(":checked")) {
                        var permission = {
                            RoleId:"@Model.RoleId",
                            Permission: chk.val(),
                            ModuleType: chk.closest("tr").find(".group-checkable").val()
                        };
                        permissions.push(permission);
                    }
                });

                return permissions;
            },
            updateButtons: function() {
                var checked = $(".checkboxes:checked").length > 0;
                var buttons = $(".batch-button");
                buttons.prop("disabled", !checked);
                if (checked) {
                    buttons.removeClass("disabled");
                } else {
                    buttons.addClass("disabled");
                }
            }
        };

        $(document).ready(function() {
            $(".group-checkable").change(function() {
                var checkAll = $(this);
                var checked = checkAll.is(":checked");
                checkAll.closest("tr").find(".checkboxes").prop("checked", checked).uniform("refresh");
                funcs.updateButtons();
            });

            $(".checkboxes").change(function() {
                funcs.updateButtons();
            });

            $('form[data-ajax-permission=true]').submit(function () {
                var permissions = funcs.getPermissions();
                var url = '@Url.Action("EditPermission", "Role")';
                var command = {
                    RolePermissions: permissions,
                    RoleId:@Model.RoleId
                    };
                postCommand(url, command);
                return false;
            });
        });
    </script>
}